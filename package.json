{
  "name": "nuxt-express-vuetify-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt.js + Express boilerplate with Vuetify component framework",
  "author": "Danijel Martinek <danijel.martinek.4@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "generate": "cross-env NODE_ENV=development HOST=0.0.0.0 PORT=3000 nuxt",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "express-session": "^1.15.3",
    "moment": "^2.20.1",
    "mongoose": "^4.10.8",
    "nuxt": "^1.3.0",
    "nuxt-oauth": "^0.0.2",
    "passport": "^0.3.2",
    "passport-strava": "latest",
    "qs": "^6.5.1",
    "source-map-support": "^0.4.15",
    "vuetify": "^1.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "backpack-core": "^0.4.3",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
